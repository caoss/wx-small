@import "../../__wuBaseWxss__/0.wxss";

page {
    height: 100%;
    position: relative;
    overflow: hidden;
    background-color: #000;
}

.body {
    height: 100%;
    width: 100%;
    position: relative;
    overflow: hidden;
}

.txt {
    position: absolute;
    top: 500px;
    width: 10000%;
    line-height: 1;
    transform-origin: 0% 50%;
    transform: rotate(90deg);
    transition-timing-function: linear;
}

.ftxt {
    position: absolute;
    width: 1000%;
    line-height: 1;
    transform-origin: 0% 50%;
    transform: rotate(90deg);
}

.move {
    top: -850px;
    transition-duration: 3s;
}

.foot-choice-col {
    position: fixed;
    bottom: 110rpx;
    left: 0;
    height: 90rpx;
    width: 100%;
    padding-top: 3px;
    padding-left: 18px;
    padding-right: 18px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    opacity: 0;
    color: #fff;
    transition-duration: 0.5s;
    z-index: 10;
}

.fluore-ft {
    justify-content: flex-end;
}

.fluore-ft view {
    margin-left: 20rpx;
}

.foot-choice-col view {
    width: 68rpx;
    height: 68rpx;
    border-radius: 50%;
    box-sizing: border-box;
    border: 2rpx solid #000;
}

.speed-choice {
    padding-left: 18%;
    padding-right: 18%;
}

.foot-choice-font view {
    width: 68rpx;
    height: 68rpx;
    font-size: 24rpx;
    text-align: center;
    line-height: 68rpx;
    border-radius: 50%;
    box-sizing: border-box;
    background-color: #333;
    border: 2rpx solid #000;
}

.foot {
    position: fixed;
    bottom: 0;
    left: 0;
    height: 98rpx;
    width: 100%;
    display: flex;
    box-sizing: border-box;
}

.ftset {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 360rpx;
    background-color: rgba(26,26,26,0.59);
    z-index: 100;
    transition-duration: 0.5s;
    transition-timing-function: ease;
}

.b440 {
    bottom: -440rpx;
    transition-duration: 0.5s;
    transition-timing-function: ease;
}

.ftset .done {
    position: absolute;
    right: 30rpx;
    top: 29rpx;
    width: 120rpx;
    height: 54rpx;
    font-size: 30rpx;
    border-radius: 100rpx;
    text-align: center;
    line-height: 54rpx;
    color: #fff;
    border: 2rpx solid #fff;
    background-color: #000;
}

.ftset .row {
    margin-top: 50rpx;
    display: flex;
    align-items: center;
    color: #fff;
}

.ftset .row:first-child {
    margin-top: 94rpx;
}

.ftset .row text {
    font-size: 32rpx;
    margin-left: 30rpx;
    margin-right: 48rpx;
}

.ftset .row view {
    width: 68rpx;
    height: 68rpx;
    font-size: 26rpx;
    text-align: center;
    line-height: 68rpx;
    border-radius: 50%;
    box-sizing: border-box;
    background-color: #333;
    border: 2rpx solid rgba(26,26,26,0.59);
}

.ftset .row view+view {
    margin-left: 34rpx;
}

